package be.businesstraining.question_9;

import javax.swing.text.html.HTMLDocument.HTMLReader.ParagraphAction;

public class Person {
	int age;
	boolean marie;
	Person epouse;
	
	public Person() {
	}
	
	public static void main(String[] args) {
		Person p = new Person();
		System.out.println("Age: "+p.age+ " Marié: "+p.marie+ "Epouse: "+ p.epouse);
	}

}
