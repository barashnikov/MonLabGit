package be.businesstraining.egalité;

public class Demo_Egalite {

	public static void main(String[] args) {
		
		Integer i1 = 10; // entre [-128,127]
		Integer i2 = 10;  // entre [-128,127]
		System.out.println(" Entiers : "+ (i1==10));
		
		Integer i3 = 128;  // En dehors de [-128,127]
		Integer i4 = 128;  // En dehors de [-128,127]
		System.out.println(" Entiers : "+ (i3==i4));

		
		
//		String s1 = "Bonjour";
//		String s2 = "Bonjour";
//		System.out.println(" Strings : "+ (s1==s2));
	}

}
