package be.businesstraining.question_4;

public class Feline {

	public static void main(String[] args) {

		int i = 111;
		boolean ok = true;

		if (ok) {
			i++;
		} else {
			i--;
		}

		System.out.println(11 + 22 + " Bonjour");

		System.out.println("Hello " + 11 + 22);

		System.out.println(44 + 55);

	}

}
