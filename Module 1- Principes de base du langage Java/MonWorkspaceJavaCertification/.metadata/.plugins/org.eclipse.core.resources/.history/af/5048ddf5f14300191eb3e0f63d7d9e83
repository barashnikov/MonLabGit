package be.businesstraining.question_4;

enum Animals {
	DOG("woof"),
	CAT("meow"),
	FISH("burble");
	
	int sound;
	private Animals(int s) {
		sound = s;
	}
}

public class TestEnum {
	static Animals a;
	public static void main(String[] args) {
		System.out.println(a.DOG.sound +  " - "+ a.FISH.sound);
	}

}
